  <script type="text/javascript">
    $(function () {
      $('#datetimepicker4').datetimepicker();
    });
    $(document).key('ctrl+shift+f', function () {
      $('#formModal').modal();
    });
    $(document).key('ctrl+shift+h', function () {
      var origin_link = $(location).attr('host');
      window.open('http://' + origin_link + '/posts/howtomarkdown');
      //window.open('https://www.google.com');
    });
    autosize(document.getElementById('create_post_textarea'));
    $.get('/posts/markdown', function (data) {
      var content = data;
      //var converter = new showdown.Converter();
      var converter = new showdown.Converter({ extensions: ['youtube'], tables: true });
      var md_help = converter.makeHtml(content);
      $('#helpmarkdown').append(md_help);
      var t = $('table').addClass('table')
    });
    key_id = "";
    $("#btnAdd").click(function(){
      current_value = $.trim($("#list_questions").val());
      $("#list_questions").val($.trim(current_value+"\r\n"+key_id));
    });
    $("#changebox").on("input", function(){
      data = $("#changebox").val();
      //console.log(data);
      $.post( "/jquery", { key: data }, function(result){
        $("#qtResult").val("");
        temp = "";
        cont = "";
        result.forEach(function(item){
          console.log(item.content)
            temp += item.id + "\r\n";
            cont += item.content +" ["+item.id+"]"+ "\r\n";
        });
        $("#qtResult").val(cont);
        key_id = $.trim(temp);
      });
    });
  </script>
  <script>
        $("#submit-btn").click(function(){
                var element = $(".custom-control-label");
                var arrayAnswer = [];
                var checkbox = $(".custom-control-input");
                for(var i=0;i<checkbox.length/4;i++){
                    for(var j=0;j<4;j++){
                        if(checkbox[i*4+j].checked==true){
                            arrayAnswer[i]=element[i*4+j].id;
                        }
                    }
                    if(arrayAnswer[i]==null){
                        arrayAnswer[i]="x";
                    }
                }
                $.post('/test_papers/final',
                        {answers: arrayAnswer},
                    );
                
        });
    </script>
